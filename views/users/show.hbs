<h1>My Profile</h1>
<h3>{{user.fullName}}</h3>

<a href="/users" class="btn btn-default">Main Page</a>
<a href="/users/{{user.id}}/edit" class="btn btn-danger">Edit Profile</a>
<a href="/users/{{user.id}}/posts" class="mc-button mc-button-text">My Post</a>

<form method="post" action="/sessions/?_method=DELETE">
  <div>
    <input type="submit" class="btn btn-default" value="LOG OUT"/>
  </div>
</form>

<ul>
  {{#this.favorites}}
    <li><a href="/users/{{this.id}}">{{this.first_name}}</a></li>
    <li>{{this.email}}</li>
    <br/>
  {{/this.favorites}}
</ul>
